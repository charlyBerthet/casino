<?xml version="1.0" encoding="UTF-8"?>
<panorama>
	<parameters>
	
	panoName=data/pano/machinesASous/images/1
	panHome=0
	tiltHome=-8
	zoomHome=1
	zoomMax = 1.3
	zoomMin = 0.7

	qualityMotion = low
	qualityStatic = high

	disableLoading = 1
	loaderTextColor = #FFFFFF
	loaderBackColor = #333333
	loaderText = 

	layer_2 = data/pano/machinesASous/files/hotspots.swf
	layer_4 = data/pano/machinesASous/files/glassMeter.swf
	layer_5 = data/pano/machinesASous/files/autorotator.swf
	layer_7 = data/pano/machinesASous/files/tooltips.swf

	</parameters>
	
	<autorotator>
        speed = -0.1
        interval = 30
        pause = 6000
        quality = medium
    </autorotator> 

	<hotspots>
		<global onStart="loadPreview();" loadPreview="  loadPano(panoName=data/pano/machinesASous/images/small/1,300);  onTransitionEnd=loadMain();  pano.qualityStatic=low " loadMain="  loadPano(panoName=data/pano/machinesASous/images/1,1000);  onTransitionEnd=loadHotspots(); " loadHotspots="  pano.zoom=0.7,1000;  " onEnterFullscreen="enterButton.visible=0;exitButton.visible=1;" onExitFullscreen="enterButton.visible=1;exitButton.visible=0;">
<spot id="enterButton" url="data/pano/machinesASous/files/full_screen.gif" static="1" staticX="-9.00" staticY="-10.00" salign="RB" align="RB" depth="51" alpha="1" onClick="fullscreen()"/>
<spot id="exitButton" url="data/pano/machinesASous/files/small_screen.gif" static="1" staticX="-9.00" staticY="-9.00" salign="RB" align="RB" depth="51" alpha="1" visible="0" onClick="fullscreen()"/>


<spot id="copy" visible="1" static="1" salign="BL" staticX="65" staticY="-30" alpha="0.4" onOver="alpha=1" onOut="alpha=0.5" url="data/pano/machinesASous/files/casino-megeve.png"/>

<spot id="aide" visible="1" static="1" pan="-50" staticY="100" tilt="0" url="nothing"/>
	
		</global>
    </hotspots>
</panorama>